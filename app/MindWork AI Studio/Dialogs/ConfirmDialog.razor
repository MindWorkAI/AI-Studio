<MudDialog>
    <DialogContent>
        <MudText Typo="Typo.body1">@this.Message</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="@this.Cancel" Variant="Variant.Filled">No</MudButton>
        <MudButton OnClick="@this.Confirm" Variant="Variant.Filled" Color="Color.Error">Yes</MudButton>
    </DialogActions>
</MudDialog>