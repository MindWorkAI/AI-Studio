@if (this.IssuesData.Any())
{
    <MudPaper Class="pr-2 mt-3" Outlined="@true">
        <MudText Typo="Typo.h6">Issues</MudText>
        <MudList T="string">
            @foreach (var issue in this.IssuesData)
            {
                <MudListItem Icon="@Icons.Material.Filled.Error" IconColor="Color.Error">
                    @issue
                </MudListItem>
            }
        </MudList>
    </MudPaper>
}